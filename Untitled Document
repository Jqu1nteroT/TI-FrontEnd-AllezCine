        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        <script src="js/bootstrap.min.js"></script>
                    
                    
                    <script>
               function buttonDisplay(classe,valeur){
                   var All=document.getElementsByClassName(classe);
                     for(var i=0;i<All.length;++i){
                        // console.log("film: "+ movies[i]);
                        All[i].style.display=valeur;
                      }     
                }
                        
            document.getElementById('All').onclick = function(){
               var classesToDiplay = ['All','Actions','Policiers','Romans', 'block'];
                for(var i=0;i<classesToDiplay.length;++i){
                        // console.log("film: "+ movies[i]);
                       var classe = classesToDiplay[i];
                   buttonDisplay(classe,'block');
                    
               }
                
            }
            
            document.getElementById('Actions').onclick = function(){
                 buttonDisplay('All','none');
                buttonDisplay('Actions','block');
                   
            }
            
            document.getElementById('Policiers').onclick = function(){
                buttonDisplay('All','none');
                buttonDisplay('Policiers','block'); 
                     
            }
            
            document.getElementById('Romans').onclick = function(){
                buttonDisplay('All','none');
                buttonDisplay('Romans','block');
          
            
            }
                    
                    </script>
        
        
       <script>
               function buttonDisplay(classe,valeur){
                   var All=document.getElementsByClassName(classe);
                     for(var i=0;i<All.length;++i){
                        // console.log("film: "+ movies[i]);
                        All[i].style.display=valeur;
                      }     
                }
                        
            document.getElementById('All-s').onclick = function(){
               var classesToDiplay = ['All-s','Actions-s','Policier-s','Romans-s', 'block'];
                for(var i=0;i<classesToDiplay.length;++i){
                        // console.log("film: "+ movies[i]);
                       var classe = classesToDiplay[i];
                   buttonDisplay(classe,'block');
                    
               }
                
            }
            
            document.getElementById('Actions-s').onclick = function(){
                 buttonDisplay('All-s','none');
                buttonDisplay('Actions-s','block');
                   
            }
            
            document.getElementById('Policiers-s').onclick = function(){
                buttonDisplay('All-s','none');
                buttonDisplay('Policiers-s','block'); 
                     
            }
            
            document.getElementById('Romans-s').onclick = function(){
                buttonDisplay('All-s','none');
                buttonDisplay('Romans-s','block');
          
            
            }
                    
                    </script>
         
          
            
